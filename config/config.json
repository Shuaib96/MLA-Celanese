{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "header-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/header/HeaderApplicationCustomizer.js",
          "manifest": "./src/extensions/header/HeaderApplicationCustomizer.manifest.json"
        }
      ]
    },
    "banner-card-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/bannerCard/BannerCardWebPart.js",
          "manifest": "./src/webparts/bannerCard/BannerCardWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HeaderApplicationCustomizerStrings": "lib/extensions/header/loc/{locale}.js",
    "BannerCardWebPartStrings": "lib/webparts/bannerCard/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}